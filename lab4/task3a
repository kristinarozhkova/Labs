import socket

HOST = '127.0.0.1'
PORT = 65433
FILENAME = "test_file.txt"

with socket.socket(socket.AF_INET, socket.SOCK_STREAM) as s:
    s.connect((HOST, PORT))
    with open(FILENAME, "rb") as f:
        data = f.read()
        s.sendall(data)
    print(f"Файл {FILENAME} відправлено на сервер")
